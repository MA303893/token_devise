<h1>New Subscription</h1>


<%= form_for :subscription, url: tenant_subscriptions_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :display_name %><br>
    <%= f.text_area :display_name %>
  </p>

  <p>
    <%= f.label :budget %><br>
    <%= f.text_area :budget %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>